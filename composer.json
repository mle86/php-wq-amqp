	"description": "An AMQP module for mle86/wq using the php-amqplib connector",
	"license": "MIT",

	"authors": [ {
		"name": "Maximilian Eul",
		"email": "maximilian@eul.cc",
		"homepage": "https://github.com/mle86/php-wq-amqp"
	} ],

	"autoload": {
